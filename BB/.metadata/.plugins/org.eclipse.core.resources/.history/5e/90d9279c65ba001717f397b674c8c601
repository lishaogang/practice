
public class time {
	public static void fun()
	{
		for (int a = 1; a < 10; a++)
			for (int b = 1; b < 10; b++)
				for (int c = 1; c < 10; c++)
					for (int d = 1; d < 10; d++)
						for (int e = 1; e < 10; e++)
							for (int f = 1; f < 10; f++)
								for (int g = 1; g < 10; g++)
									for (int h = 1; h < 10; h++)
										for (int i = 1; i < 10; i++)
											if (((1 << a) |
												(1 << b) |
												(1 << c) |
												(1 << d) |
												(1 << e) |
												(1 << f) |
												(1 << g) |
												(1 << h) |
												(1 << i)) == 0x3FE)
											{
												int x = a * 100 + b * 10 + c,
													y = d * 100 + e * 10 + f,
													z = g * 100 + h * 10 + i;
												if (y == 2 * x && z == 3 * x)
													System.out.printf("%d:%d:%d\n", x, y, z);
											}
	}
	public static void foo(){
		int x = 0;
		for (int a = 1; a < 10; a++)
			for (int b = 1; b < 10; b++)
				for (int c = 1; c < 10; c++)
					for (int d = 1; d < 10; d++)
						for (int e = 1; e < 10; e++)
							for (int f = 1; f < 10; f++)
								for (int g = 1; g < 10; g++)
									for (int h = 1; h < 10; h++)
										for (int i = 1; i < 10; i++)
											x++;
	}
	public static void main(String []args){
		long start = System.currentTimeMillis();
		fun();
		long end = System.currentTimeMillis();
		System.out.printf("run time: %dms\n", (end - start));
	}
}
